{
  "WindowSize": { //размер окна
    "Width": 0, //отвечает за ширину окна, при значении 0 ориентируется на разрешение экрана
    "Hight": 0  //отвечает за высоту окна, при значении 0 ориентируется на разрешение экрана
  },
  "Bot": { //настройки игроков\ботов
    "IsWhiteBot": false, //является белые ботом\подконтрольным игроком
    "IsBlackBot": true, //является черные ботом\подконтрольным игроком
    "WhiteBotLevel": 0, //расчитывает на сколько ходов вперед расчитывает свой ход бот белый
    "BlackBotLevel": 5, //расчитывает на сколько ходов вперед расчитывает свой ход бот черный
    "BotScoringType": "NumberAndPotential", //параметр, который влияет на выбор последующего хода
    "BotDelayMS": 0, //задержка хода бота в милисекундах
    "NoRandom": false, //будет использовать случайный порядок чисел, либо зависящий от времени при случайном выборе кода из найденных 
    "Optimization": "O1" //оптимизирует ходы, при значени не 00 выходим из цикла поиска наилучшего хода
  },
    "Game": { //параметры игры
        "MaxNumTurns": 120 //максимальное количество ходов в партии
    }
